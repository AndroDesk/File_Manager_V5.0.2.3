<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="com.android.fileexplorer"
    platformBuildVersionCode="33"
    platformBuildVersionName="13"
    android:compileSdkVersion="33"
    android:compileSdkVersionCodename="13"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Izinkan apl membaca setelan Layar utama dan pintasan. -->
    <uses-permission
        android:name="com.android.launcher.permission.READ_SETTINGS" />
    <!-- Izinkan apl mengubah setelan Layar utama dan pintasan. -->
    <uses-permission
        android:name="com.android.launcher.permission.WRITE_SETTINGS" />
    <uses-permission
        android:name="android.permission.GET_APP_GRANTED_URI_PERMISSIONS" />
    <!-- Memungkinkan aplikasi melihat informasi tentang jaringan Wi-Fi, misalnya apakah Wi-Fi diaktifkan dan nama perangkat Wi-Fi yang terhubung. -->
    <uses-permission
        android:name="android.permission.ACCESS_WIFI_STATE" />
    <!-- Memungkinkan aplikasi membuat soket jaringan dan menggunakan protokol jaringan khusus. Browser dan aplikasi lain menyediakan sarana untuk mengirim data ke internet sehingga izin ini tidak diperlukan untuk mengirim data ke internet. -->
    <uses-permission
        android:name="android.permission.INTERNET" />
    <!-- Memungkinkan aplikasi menyambung ke dan memutus dari titik akses Wi-Fi, dan mengubah konfigurasi perangkat untuk jaringan Wi-Fi. -->
    <uses-permission
        android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission
        android:name="android.permission.WRITE_SECURE_SETTINGS" />
    <!-- Mengizinkan aplikasi menulis konten penyimpanan bersama Anda. -->
    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- Mengizinkan aplikasi membaca konten penyimpanan bersama Anda. -->
    <uses-permission
        android:name="android.permission.READ_EXTERNAL_STORAGE"
        android:maxSdkVersion="32" />
    <uses-permission
        android:name="android.permission.DELETE_PACKAGES" />
    <!-- Mengizinkan apl mencegah ponsel tidur. -->
    <uses-permission
        android:name="android.permission.WAKE_LOCK" />
    <!-- Memungkinkan aplikasi melihat informasi tentang koneksi jaringan, misalnya jaringan yang ada dan terhubung. -->
    <uses-permission
        android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!-- Memungkinkan aplikasi mendapatkan daftar akun yang dikenal oleh ponsel. Ini mungkin termasuk akun yang dibuat oleh aplikasi yang telah Anda instal. -->
    <uses-permission
        android:name="android.permission.GET_ACCOUNTS" />
    <!-- Mengizinkan apl mengambil kode, data, dan ukuran temboloknya -->
    <uses-permission
        android:name="android.permission.GET_PACKAGE_SIZE" />
    <uses-permission
        android:name="android.permission.DELETE_CACHE_FILES" />
    <uses-permission
        android:name="android.permission.USE_CREDENTIALS" />
    <uses-permission
        android:name="android.permission.MANAGE_ACCOUNTS" />
    <uses-permission
        android:name="android.permission.READ_PRIVILEGED_PHONE_STATE"
        android:maxSdkVersion="28" />
    <uses-permission
        android:name="android.permission.GET_ACCOUNTS_PRIVILEGED"
        android:maxSdkVersion="28" />
    <uses-permission
        android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <!-- Mengizinkan aplikasi menambahkan pintasan Layar Utama tanpa tindakan dari pengguna. -->
    <uses-permission
        android:name="com.android.launcher.permission.INSTALL_SHORTCUT" />
    <!-- Memungkinkan aplikasi mengambil informasi tentang tugas yang dijalankan saat ini dan baru-baru ini. Izin ini memungkinkan aplikasi menemukan informasi tentang aplikasi mana yang digunakan pada perangkat. -->
    <uses-permission
        android:name="android.permission.GET_TASKS" />
    <!-- Mengizinkan aplikasi untuk mengendalikan vibrator. -->
    <uses-permission
        android:name="android.permission.VIBRATE" />
    <uses-permission
        android:name="android.permission.WRITE_MEDIA_STORAGE" />
    <uses-permission
        android:name="android.permission.ACCESS_ALL_EXTERNAL_STORAGE" />
    <uses-permission
        android:name="android.permission.MANAGE_USERS" />
    <uses-permission
        android:name="android.permission.INTERACT_ACROSS_USERS" />
    <uses-permission
        android:name="com.xiaomi.permission.AUTH_SERVICE" />
    <!-- Mengizinkan apl memodifikasi data setelan sistem. Apl berbahaya dapat merusak konfigurasi sistem anda. -->
    <uses-permission
        android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission
        android:name="android.permission.ACCESS_KEYGUARD_SECURE_STORAGE" />
    <uses-permission
        android:name="com.miui.cleanmaster.permission.InstallCleanMaster" />
    <!-- Mengizinkan aplikasi meminta pemasangan paket. -->
    <uses-permission
        android:name="android.permission.REQUEST_INSTALL_PACKAGES" />
    <uses-permission
        android:name="android.permission.MANAGE_EXTERNAL_STORAGE" />
    <!-- Mengizinkan aplikasi melihat semua paket yang diinstal. -->
    <uses-permission
        android:name="android.permission.QUERY_ALL_PACKAGES" />
    <!-- Aplikasi ini dapat muncul di atas aplikasi lain atau bagian lain pada layar. Ini dapat mengganggu penggunaan aplikasi biasa dan mengubah kemunculan aplikasi lainnya. -->
    <uses-permission
        android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <!-- Mengizinkan aplikasi membaca file gambar dari penyimpanan bersama. -->
    <uses-permission
        android:name="android.permission.READ_MEDIA_IMAGES" />
    <!-- Mengizinkan aplikasi membaca file audio dari penyimpanan bersama. -->
    <uses-permission
        android:name="android.permission.READ_MEDIA_AUDIO" />
    <!-- Mengizinkan aplikasi membaca file video dari penyimpanan bersama. -->
    <uses-permission
        android:name="android.permission.READ_MEDIA_VIDEO" />
    <!-- Mengizinkan aplikasi untuk menampilkan notifikasi -->
    <uses-permission
        android:name="android.permission.POST_NOTIFICATIONS" />
    <uses-permission
        android:name="com.miui.onehop.permission.MIRROR" />
    <uses-permission
        android:name="android.permission.SET_PREFERRED_APPLICATIONS" />
    <permission
        android:name="com.android.fileexplorer.permission.DIRPARSE"
        android:protectionLevel="signatureOrSystem" />
    <permission
        android:name="com.android.fileexplorer.permission.ACCESS_FILE_CONTENT"
        android:protectionLevel="signatureOrSystem" />
    <uses-permission
        android:name="android.permission.READ_MEDIA_VISUAL_USER_SELECTED" />
    <uses-permission
        android:name="com.miui.securitycenter.permission.PERM_USE_INFO" />
    <uses-permission
        android:name="com.xiaomi.gallery.permission.CLOUD" />
    <uses-permission
        android:name="com.miui.gallery.permission.USE_API" />
    <uses-permission
        android:name="com.android.fileexplorer.permission.DIRPARSE" />
    <uses-permission
        android:name="com.android.fileexplorer.permission.ACCESS_FILE_CONTENT" />
    <uses-permission
        android:name="com.cleanmaster.permission.sdk.clean" />
    <uses-permission
        android:name="miui.permission.USE_INTERNAL_GENERAL_API" />
    <uses-permission
        android:name="com.mi.android.globallauncher.permission.READ_SETTINGS" />
    <uses-permission
        android:name="com.mi.android.globallauncher.permission.WRITE_SETTINGS" />
