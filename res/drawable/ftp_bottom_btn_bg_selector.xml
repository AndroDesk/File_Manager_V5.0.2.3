<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_enabled="true">
        <shape
            android:shape="rectangle">
            <corners
                android:radius="16.0dip" />
            <solid
                android:color="@color/common_color_blue" />
        </shape>
    </item>
    <item
        android:state_enabled="false">
        <shape
            android:shape="rectangle">
            <corners
                android:radius="16.0dip" />
            <solid
                android:color="@color/common_default_image_color" />
        </shape>
    </item>
    <item>
        <shape
            android:shape="rectangle">
            <corners
                android:radius="16.0dip" />
            <solid
                android:color="@color/common_color_blue" />
        </shape>
    </item>
</selector>