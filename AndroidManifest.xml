        android:name="com.miui.securitycenter.permission.SYSTEM_PERMISSION_DECLARE" />
    <uses-permission
        android:name="cn.wps.moffice.lite.permission.BIND_SNAPSHOT_SERVICE" />
    <!-- Mengizinkan aplikasi menggunakan layanan di latar depan. -->
    <uses-permission
        android:name="android.permission.FOREGROUND_SERVICE" />
